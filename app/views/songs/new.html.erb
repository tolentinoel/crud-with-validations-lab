<%= form_for @song do |f| %>
     <%= f.label :title%>
    <%= f.text_field :title%>

    <%= f.label :artist_name%>
    <%= f.text_field :artist_name%>

    <label>Song released:</label><br>
    <%= f.text_field :released %><br>

    <%= f.label :release_year%>
    <%= f.text_field :release_year%>

    <%= f.label :genre%>
    <%= f.text_field :genre%>


  <%= f.submit %>
<% end %>